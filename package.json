{
  "name": "@battis/gas-lighter",
  "version": "0.1.0",
  "author": "Seth Battis <seth@battis.net>",
  "license": "GPL-3.0",
  "main": "./src/index.js",
  "types": "./src/types.d.ts",
  "homepage": "https://github.com/battis/gas-lighter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battis/gas-lighter.git"
  },
  "bugs": {
    "url": "https://github.com/battis/gas-lighter/issues"
  },
  "scripts": {
    "dev": "git add worktree publish publish",
    "update-dependencies": "pnpm update --shamefully-hoist && npm update --package-lock-only",
    "transfer-package-to-publish": "copyfiles package.json package-lock.json pnpm-lock.yaml publish",
    "update": "npm run update-dependencies && npm run transfer-package-to-publish",
    "clean": "rimraf publish/src",
    "copy-files": "copyfiles -u 1 \"src/**/*.html\" publish/src",
    "build": "npm run clean && tsc && npm rum copy-files",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/google-apps-script": "^1.0.57",
    "@types/node": "^18.11.19",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@battis/eslint-config": "github:battis/eslint-config",
    "@battis/prettier-config": "github:battis/prettier-config",
    "@tsconfig/recommended": "^1.0.2",
    "@types/google-apps-script": "^1.0.57",
    "@types/node": "^18.14.1",
    "copyfiles": "^2.4.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "prettier": "@battis/prettier-config",
  "eslintConfig": {
    "extends": [
      "@battis/eslint-config"
    ]
  }
}
