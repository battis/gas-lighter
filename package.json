{
  "name": "@battis/gas-lighter",
  "version": "0.1.2",
  "author": "Seth Battis <seth@battis.net>",
  "license": "GPL-3.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/battis/gas-lighter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battis/gas-lighter.git"
  },
  "bugs": {
    "url": "https://github.com/battis/gas-lighter/issues"
  },
  "engines": {
    "node": "^12.17"
  },
  "scripts": {
    "dev": "run-s dev:*",
    "dev:docs-worktree": "git worktree add docs docs",
    "docs": "run-s docs:**",
    "docs:clean": "shx rm -rf docs/*",
    "docs:build": "npx typedoc",
    "docs:build:open": "./scripts/docs/launch.mjs",
    "clean": "shx rm -rf dist",
    "build": "run-s build:*",
    "build:clean": "npm run clean",
    "build:transpile": "tsc --project tsconfig.build.json",
    "build:html": "copyfiles -u 1 \"src/**/*.html\" dist",
    "deploy": "run-s deploy:**",
    "deploy:build": "npm run build",
    "deploy:publish": "np --yolo --omit=dev",
    "deploy:docs:build": "run-s docs:*",
    "deploy:docs:publish": "./scripts/docs/publish.mjs"
  },
  "peerDependencies": {
    "@google/clasp": "^2.4.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/google-apps-script": "^1.0.57",
    "@types/node": "^18.11.19",
    "gas-webpack-plugin": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "devDependencies": {
    "@battis/eslint-config": "latest",
    "@battis/prettier-config": "latest",
    "@google/clasp": "^2.4.2",
    "@tsconfig/recommended": "^1.0.2",
    "@types/google-apps-script": "^1.0.64",
    "@types/node": "^18.16.19",
    "app-root-path": "^3.1.0",
    "copyfiles": "^2.4.1",
    "gas-webpack-plugin": "^2.5.0",
    "np": "^8.0.4",
    "open": "^9.1.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-loader": "^9.4.4",
    "typedoc": "^0.23.28",
    "typescript": "^4.9.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  },
  "prettier": "@battis/prettier-config"
}
