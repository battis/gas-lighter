    "@tsconfig/recommended": "^1.0.7",
    "lerna": "^8.1.8",
    "syncpack": "^12.4.0",
    "typescript": "^4.9.5"
  },
  "prettier": "@battis/prettier-config",
  "eslintConfig": {
    "extends": "@battis/eslint-config",
    "overrides": [
      {
        "excludedFiles": [
          "**/webpack.config.ts"
        ],
        "env": {
          "browser": true,
          "node": false
        }
      }
    ]
  }
}
