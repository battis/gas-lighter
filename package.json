{
  "name": "@battis/gas-lighter",
  "version": "0.4.4",
  "author": "Seth Battis <seth@battis.net>",
  "license": "GPL-3.0",
  "main": "./src/index.ts",
  "homepage": "https://github.com/battis/gas-lighter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/battis/gas-lighter.git"
  },
  "bugs": {
    "url": "https://github.com/battis/gas-lighter/issues"
  },
  "engines": {
    "node": ">=12.17"
  },
  "bin": {
    "setup": "./bin/setup.mjs"
  },
  "scripts": {
    "dev": "run-s dev:*",
    "dev:docs-worktree": "git worktree add docs docs",
    "docs": "run-s docs:**",
    "docs:clean": "shx rm -rf docs/*",
    "docs:build": "typedoc --tsconfig ./tsconfig.typedoc.json --plugin typedoc-plugin-missing-exports",
    "docs:build:open": "node ./bin/docs/launch.mjs",
    "build": "run-s build:*",
    "build:webpack": "webpack",
    "build:wrap": "node --no-warnings ./bin/build/wrap.mjs",
    "release": "run-s release:**",
    "release:build": "npm run build",
    "release:publish": "np --yolo --no-release-draft",
    "release:docs:build": "run-s docs:*",
    "release:docs:publish": "node ./bin/docs/publish.mjs"
  },
  "dependencies": {
    "@battis/qui-cli": "^0.3.6",
    "@battis/typescript-tricks": "^0.4.2",
    "@effortlessmotion/html-webpack-inline-source-plugin": "^1.0.3",
    "@google/clasp": "^2.4.2",
    "@tsconfig/recommended": "^1.0.3",
    "@types/google-apps-script": "^1.0.75",
    "@types/node": "^18.18.5",
    "app-root-path": "^3.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "gas-webpack-plugin": "^2.5.0",
    "html-loader": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.2.0",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.5.0",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@battis/eslint-config": "latest",
    "@battis/prettier-config": "latest",
    "np": "^8.0.4",
    "open": "^9.1.0",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "typedoc": "^0.25.2",
    "typedoc-plugin-missing-exports": "^2.1.0"
  },
  "prettier": "@battis/prettier-config"
}
